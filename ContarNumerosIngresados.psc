Algoritmo ContarNumerosIngresados 
	Definir cont, i, n, acu Como Entero
	i=1;
	acu =0;
	cont =0;
	Mientras i>=0 Hacer
		Escribir "Ingresa un valor ";
		Leer n;
		acu = acu+n
		con=cont +1;
		i=n;
	FinMientras
	Escribir "Los numeros ingresados son: " cont;
	Escribir "El valor acumulado es: " acu ;
FinAlgoritmo
