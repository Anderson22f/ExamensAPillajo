Algoritmo Ejecicio
	Definir nro, lista, cont Como Entero;
	Definir media Como Real
	
	Escribir "Ingresa el numero";
	Leer nro;
	
	lista=0;
	cont=0;
	media=0;
	Mientras nro>0 Hacer
		lista = lista + nro ;
		cont = cont + 1;
		Leer nro;
	Fin Mientras
	
	media = lista / cont ;
	Imprimir " La media de la lista de numeros positivos es: ", media;
	
	
FinAlgoritmo
